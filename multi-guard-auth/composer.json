{
    "name": "bro/multi-guard-auth",
    "description": "Laravel package for creating multiple authentication guards",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ebrahem Alwish",
            "email": "ebrahemk968@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "bro/multi-guard-auth": "^1.0",
        "laravel/framework": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "multi-guard-auth\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "multi-guard-auth\\src\\MultiGuardAuthServiceProvider"
            ]
        }
    },
    "repositories": [
    {
        "type": "vcs",
        "url": "https://github.com/ebrahem-eng/MultiGuard-Breeze.git"
    }
],
    "minimum-stability": "dev",
    "prefer-stable": true
}
