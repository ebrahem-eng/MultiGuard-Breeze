{
    "name": "bro/multi-guard-auth",
    "description": "Laravel package for creating multiple authentication guards",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ebrahem Alwish",
            "email": "ebrahemk968@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Bro\\multi-guard-auth\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bro\\multi-guard-auth\\src\\MultiGuardAuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
